{
  "expo": {
    "name": "AgriAssist",
    "slug": "agriassist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "scheme": "agriassist",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.agriassist.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png",
        "backgroundColor": "#16543a"
      },
      "package": "com.agriassist.app",
      "navigationBar": {
        "visible": false,
        "backgroundColor": "transparent",
        "height": 0
      },
      "softwareKeyboardLayoutMode": "pan",
      "statusBar": {
        "hidden": true,
        "backgroundColor": "transparent"
      },
      "edgeToEdge": false
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-navigation-bar",
        {
          "backgroundColor": "transparent",
          "height": 0,
          "visible": false
        }
      ]
    ],
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "eas": {
        "projectId": "3a3b8e5f-c66b-49dc-8f6a-457a9ecd23e0"
      }
    }
  }
}
